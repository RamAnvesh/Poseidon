{
  "url": "/v1/arithmetic",
  "httpMethod": "GET",
  "timeout": 10000,
  "name": "arithmetic",
  "params": {
    "required": [
      {
        "name": "num1",
        "datatype": "number"
      },
      {
        "name": "num2",
        "datatype": "number"
      }
    ]
  },
  "tasks": {
    "addResultDT": {
      "name": "ArithmeticDS_1.0.0",
      "context": {
        "num1": "{{$num1}}",
        "num2": "{{$num2}}",
        "operation": "add"
      }
    },
    "subtractResultDT": {
      "name": "ArithmeticDS_1.0.0",
      "context": {
        "num1": "{{$num1}}",
        "num2": "{{$num2}}",
        "operation": "subtract"
      }
    },
    "multiplyResultDT": {
      "name": "ArithmeticDS_1.0.0",
      "context": {
        "num1": "{{$num1}}",
        "num2": "{{$num2}}",
        "operation": "multiply"
      }
    },
    "divideResultDT": {
      "name": "ArithmeticDS_1.0.0",
      "context": {
        "num1": "{{$num1}}",
        "num2": "{{$num2}}",
        "operation": "divide"
      }
    }
  },
  "response": {
    "add": "{{$addResultDT.result}}",
    "subtract": "{{$subtractResultDT.result}}",
    "multiply": "{{$multiplyResultDT.result}}",
    "divide": "{{$divideResultDT.result}}"
  }
}